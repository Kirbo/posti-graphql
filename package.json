{
  "name": "posti-graphql",
  "version": "0.1.1",
  "description": "Posti Basic Address auto-updater GraphQL server",
  "repository": "https://github.com/kirbo/posti-graphql",
  "author": "Kimmo Saari <kirbo@kirbo-designs.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:schema && cross-env NODE_ENV=build npx babel src --out-dir dist",
    "build:schema": "babel-node src/build.js",
    "start": "babel-node src/server.js",
    "start:dev": "nodemon --ignore src/graphql/Types/ -x npm-run-all build:schema start:server",
    "start:server": "babel-node src/server.js",
    "force": "cross-env force=true babel-node src/posti.js",
    "lint": "eslint .",
    "test": "yarn test:debug --silent",
    "test:debug": "cross-env NODE_ENV=test jest --verbose --forceExit",
    "prepush": "yarn lint && yarn test"
  },
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.3.2"
  },
  "bin": {
    "posti-server": "./dist/server.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "posti",
    "finnish post",
    "basic addresses",
    "graphql"
  ],
  "dependencies": {
    "apollo-engine": "1.0.3",
    "apollo-server-express": "1.3.2",
    "compression": "1.7.2",
    "express": "4.16.3",
    "express-rate-limit": "2.11.0",
    "fs-extra": "5.0.0",
    "graphiql": "0.11.11",
    "graphql": "0.13.2",
    "graphql-fields": "1.0.2",
    "graphql-playground-middleware-express": "1.5.8",
    "graphql-tools": "2.23.1"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.40",
    "@babel/core": "7.0.0-beta.40",
    "@babel/polyfill": "7.0.0-beta.40",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-minify-constant-folding": "0.3.0",
    "babel-plugin-minify-dead-code-elimination": "0.3.0",
    "babel-plugin-minify-guarded-expressions": "0.3.0",
    "babel-plugin-minify-mangle-names": "0.3.0",
    "babel-plugin-minify-replace": "0.3.0",
    "babel-plugin-minify-simplify": "0.3.0",
    "babel-plugin-remove-comments": "2.0.0",
    "babel-plugin-shebang": "0.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-property-literals": "6.9.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-minify": "0.3.0",
    "babel-preset-stage-2": "6.24.1",
    "cross-env": "5.1.4",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-compat": "2.2.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsdoc": "3.5.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-require-jsdoc": "1.0.4",
    "husky": "0.14.3",
    "jest": "22.4.2",
    "jest-plugin-console-matchers": "2.7.0",
    "nodemon": "1.17.2",
    "npm-run-all": "4.1.2",
    "posti": ">=1.0.1"
  },
  "peerDependencies": {
    "posti": ">=1.0.1"
  }
}
